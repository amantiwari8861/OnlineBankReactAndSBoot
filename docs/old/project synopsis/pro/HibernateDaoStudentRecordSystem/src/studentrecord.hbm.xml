<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="beans.StudentRecord" table="student_record">
<id name="rollno" column="roll_no">
<generator class="increment"></generator>
</id>
<property name="name" column="name"></property>
<property name="email" column="email"></property>
<property name="grade" column="grade"></property>
<property name="phy" column="physics"></property>
<property name="chem" column="chemistry"></property>
<property name="maths" column="maths"></property>
<property name="total" column="total_marks"></property>
</class>
</hibernate-mapping>